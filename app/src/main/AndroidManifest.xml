<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.dbtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddNoteActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".UnitConverterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".UpdateStudentActivity" />
        <activity android:name=".ModifyStudentActivity" />
        <activity android:name=".ViewStudentActivity" />
        <activity android:name=".AddStudent" />
        <activity android:name=".Settings" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>